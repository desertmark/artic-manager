    "awilix-express": "^1.1.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.2.2",
    "lodash": "^4.17.10",
    "mongoose": "^5.1.3",
    "passport": "^0.4.0",
